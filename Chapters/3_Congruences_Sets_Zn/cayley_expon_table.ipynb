   "source": [
    "# Tables Mod n\n",
    "# https://crypto.katestange.net/multiplication-tables-modulo-n/\n",
    "\n",
    "n = 5\n",
    "R = IntegerModRing(n)\n",
    "U = IntegerModRing(euler_phi(n))\n",
    "rows = [['^']+[str(r) for r in U]]+[[i]+[i^r for r in U] for i in R]\n",
    "table(rows)"
   ]
