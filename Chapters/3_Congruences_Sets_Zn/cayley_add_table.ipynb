    "# Tables Mod n\n",
    "# https://crypto.katestange.net/multiplication-tables-modulo-n/\n",
    "\n",
    "n = 5\n",
    "R = IntegerModRing(n)\n",
    "rows = [['*']+[str(r) for r in R]]+[[i]+[i+r for r in R] for i in R]\n",
    "table(rows)"
   ]
